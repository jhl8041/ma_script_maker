{
    "name": "MA Script Maker",
    "version": "1.00",
    "description": "Create custom MA script",
    "permissions": [
        "activeTab",
        "declarativeContent",
        "storage"
    ],
    "content_scripts": [
       {   
          "matches": ["http://*/*", "https://*/*"],
          "css": ["embed.css"]
       }
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
   "manifest_version": 2
}